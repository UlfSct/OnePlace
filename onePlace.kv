<MainWindow>:
    name: 'main'
    map: map
    on_enter:
        root.build_map()
        root.write_tasks()
    BoxLayout:
        orientation: 'vertical'
        MDToolbar:
            padding: 0, 0, 18, 0
            md_bg_color: 167/255, 196/255, 128/255, 1
            MDLabel:
                id: head_label
                size_hint: None, None
                width: Window.width
                halign: 'center'
                font_name: 'Roboto'
                font_size: 40
                color: 237/255, 245/255, 235/255, 1
                text: 'Задания'
#                canvas.before:
#                    Color:
#                        rgba: 1, 0, 0, 1
#                    Rectangle:
#                        size: self.size
#                        pos: self.pos
        MDBottomNavigation:
            panel_color: 167/255, 196/255, 128/255, 1
            text_color_active: 0, 0, 0, 1
            MDBottomNavigationItem:
                on_enter: root.ids.head_label.text='Задания'
                name: 'taskWindow'
                icon: 'home'
                BoxLayout:
                    orientation: 'vertical'
                    MDLabel:
                        id: t1
                    MDLabel:
                        id: t2
                    MDLabel:
                        id: t3

