<ImageWithBack@Image>
    canvas.before:
        Color:
            rgba: 251/255, 1, 247/255, 1
        Rectangle:
            pos: self.pos
            size: self.size

<CoolTextInput@TextInput>
    background_normal: ''
    background_color: (232/255, 244/255, 217/255, 1)
    font_name: 'Roboto'
    background_normal: 'green'
    background_down: 'green'
    cursor_color: (0, 0, 0, 1)
    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
    size: 0, 70
    size_hint: 1, None
    font_size: 26
    canvas.before:
        Color:
            rgba: (99/255, 139/255, 44/255, 1)
        Line:
            width: 1
            rectangle: self.x, self.y, self.width, self.height

<PlantShopLayout@BoxLayout>
    canvas.before:
        Color:
            rgba: (1, 1, 1, 1.0)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [20]
        Color:
            rgba: (180/255, 180/255, 180/255, 1)
        Line:
            width: 0.5
            rectangle: self.x + self.width / 2, self.y, 1, self.height


<InfoLayout@FloatLayout>
