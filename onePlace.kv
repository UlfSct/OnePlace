                            Image:
                                source: './img/glass.png'
                                size_hint: 0.5, 0.5
                                pos_hint: {'center_x': 0.5, 'center_y': 0.6}
                            Label:
                                text: 'Стекло'
                                color: 'black'
                                font_name: 'Verdana'
                                font_size: 23
                                size_hint: 0.5, 0.5
                                pos_hint: {'center_x': 0.5, 'center_y': 0.2}
                            Button:
                                background_color: 0, 0, 0, 0
                                size_hint: 1, 1
                                pos_hint: {'x': 0, 'y': 0}
                                on_release:
                                    app.root.current = 'glass'
                        InfoLayout:
                            Image:
                                source: './img/paper.png'
                                size_hint: 0.5, 0.5
                                pos_hint: {'center_x': 0.5, 'center_y': 0.6}
                            Label:
                                text: 'Бумага'
