<RoundedButton@Button>
    font_name: 'Roboto'
    font_size: 28
    background_normal: ''
    background_color: (0,0,0,0)
    canvas.before:
        Color:
            rgba: (127/255, 178/255, 57/255, 1.0) if self.state == 'normal' else (127/255, 192/255, 57/255, 1.0)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [25]

<Header@Label>
    background_normal: ''
    background_color: (0, 0, 0, 1)
    font_name: 'Roboto'
    font_size: 40
    canvas.before:
        Color:
            rgba: (127/255, 178/255, 57/255, 1.0)
        Rectangle:
            size: self.size
            pos: self.pos

<BlackColorLabel@Label>
    color: 'black'
    font_name: 'Roboto'
    font_size: 23

<BoxBackLayout@BoxLayout>
    canvas.before:
