    RegWindow:
    FailedRegWindow:
    SuccessRegWindow:
    MainWindow:
    PlasticWindow:
    GlassWindow:
    PaperWindow:
    TetraPackWindow:
    MetalWindow:
    BatteryWindow:


<PlasticWindow@Screen>:
    canvas.before:
        Color:
            rgba: 251/255, 1, 247/255, 1
        Rectangle:
            pos: self.pos
            size: self.size

<GlassWindow@Screen>:
