                        rows: 6
                        size_hint: 1, None
                        size: 0, 1560
                        spacing: 30
                        padding: 50, 30
                        pos_hint: {'center_x': 0.5}
                        InfoLayout:
                            Image:
                                source: './img/plastic.png'
                                size_hint: 0.5, 0.5
                                pos_hint: {'center_x': 0.5, 'center_y': 0.6}
                            Label:
                                text: 'Пластик'
                                color: 'black'
                                font_name: 'Verdana'
